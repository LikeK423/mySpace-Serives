{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "app/*": ["src/app/*"],
      "constants": ["src/constants"],
      "controller/*": ["src/controller/*"],
      "handle/*": ["src/handle/*"],
      "middleware/*":["src/middleware/*"],
      "#page": ["src/page/*"],
      "request/*": ["src/request/*"],
      "router/*": ["src/router/*"],
      "service/*": ["src/service/*"],
      "utils/*": ["src/utils/*"]
    }
  }
}